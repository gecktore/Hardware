<Window x:Class="Hardware.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hardware"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="360*"/>
        </Grid.RowDefinitions>
        <Rectangle Fill="PeachPuff" Grid.ColumnSpan="2" />
        <Image Source="/res/telek.png" HorizontalAlignment="Left" Height="20"/>
        <TextBlock x:Name="TitleTb" HorizontalAlignment="Center" FontSize="15">Магазин</TextBlock>
        <Frame NavigationUIVisibility="Hidden" x:Name="MyFrame" Grid.Row="1"></Frame>
    </Grid>
</Window>
